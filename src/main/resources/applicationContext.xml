<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <bean id="huang" class="com.zj.j2eehomework.zuoye11.HuangTeacher"/>
    <bean id="green" class="com.zj.j2eehomework.zuoye11.GreenTeacher"/>
    <bean id="stu" class="com.zj.j2eehomework.zuoye11.Student" scope="prototype" init-method="stuInit" destroy-method="stuDestroy">
        <constructor-arg index="0" value="李隆基"/>
        <constructor-arg index="1" value="65"/>
        <constructor-arg index="2" ref="green"/>
    </bean>
<!--需要告诉spring现在我所用组件注解了-->
    <context:component-scan base-package="com.zj.j2eehomework.zuoye11"/>
</beans>